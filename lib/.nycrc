{
    "extends": "@istanbuljs/nyc-config-typescript",
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80,
    "all": true,
    "check-coverage": false,
    "reporter": [ "text", "html" ],
    "include": [ "**/*.ts" ],
    "exclude": [ 
        "**/test/src/**/*.ts",
        "**/src/index.ts", 
        "**/src/polyfills.ts", 
        "**/types/**/*.d.ts", 
        "**/karma*.conf.js" 
    ],
    "report-dir": "../coverage/nyc",
    "temp-dir": "../.nyc_output"
}